<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backups -->
        <exclude domain="sharedpref" path="encrypted_prefs" />
        <exclude domain="database" path="owner_hub.db" />
    </cloud-backup>
    <device-transfer>
        <!-- Exclude sensitive data from device transfer -->
        <exclude domain="sharedpref" path="encrypted_prefs" />
        <exclude domain="database" path="owner_hub.db" />
    </device-transfer>
</data-extraction-rules>
